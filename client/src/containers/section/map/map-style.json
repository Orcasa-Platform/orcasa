{
  "version": 8,
  "name": "OSM Liberty",
  "metadata": {
    "maputnik:license": "https://github.com/maputnik/osm-liberty/blob/gh-pages/LICENSE.md",
    "maputnik:renderer": "mbgljs",
    "openmaptiles:version": "3.x",
    "groups": {
      "basemap-satellite": {
          "name": "basemap-satellite",
          "collapsed": true
      },
      "basemap-light": {
          "name": "basemap-light",
          "collapsed": true
      },
      "labels-light": {
          "name": "labels-light",
          "collapsed": true
      },
      "labels-dark": {
          "name": "labels-dark",
          "collapsed": true
      }
    }
  },
  "center": [-2.6399654650078617, 39.94542670903479],
  "zoom": 6.243947994328476,
  "bearing": 0,
  "pitch": 0,
  "light": {"anchor": "viewport", "color": "hsla(0, 0%, 100%, 0)"},
  "sources": {
    "basemap-satellite": {
      "type": "raster",
      "tileSize": 256,
      "tiles": [
        "https://server.arcgisonline.com/ArcGIS/rest/services/World_Imagery/MapServer/tile/{z}/{y}/{x}"
      ],
      "maxzoom": 12,
      "minzoom": 3,
      "layout": { "visibility": "none" }
    },
    "basemap-light": {
      "type": "raster",
      "tileSize": 256,
      "tiles": [
        "https://a.basemaps.cartocdn.com/light_nolabels/{z}/{x}/{y}{r}.png"
      ],
      "maxzoom": 12,
      "minzoom": 3,
      "layout": { "visibility": "none" }
    },
    "labels": {
      "data": "https://raw.githubusercontent.com/Orcasa-Platform/orcasa/main/client/public/country_labels.json",
      "type": "geojson"
    }
  },
  "sprite": "https://maputnik.github.io/osm-liberty/sprites/osm-liberty",
  "glyphs": "https://api.maptiler.com/fonts/{fontstack}/{range}.pbf?key=8k3OpweQg8O0TfOOUwcF",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "layout": {"visibility": "visible"},
      "paint": {"background-color": "#f8f4f0"}

    },
    {
      "id": "basemap-satellite",
      "type": "raster",
      "source": "basemap-satellite",
      "metadata": {"group": "basemap-satellite"},
      "layout": {"visibility": "none"}
    },
    {
      "id": "basemap-light",
      "type": "raster",
      "source": "basemap-light",
      "metadata": {"group": "basemap-light"},
      "layout": {"visibility": "none"}
    },

    {
      "id": "custom-layers",
      "type": "background",
      "layout": {"visibility": "none"},
      "paint": {"background-color": "#f8f4f0"}
    },
    {
      "id": "countries-labels",
      "type": "symbol",
      "layout": {
        "text-field": [
          "get",
          "country_name"
        ],
        "text-anchor": "top"
      },
      "source": "labels",
      "metadata": {
        "group": "labels-dark",
        "position": "top"
      }
    },
    {
      "id": "countries-labels-light",
      "type": "symbol",
      "paint": {
        "text-color": "#fff"
      },
      "layout": {
        "text-field": [
          "get",
          "country_name"
        ],
        "text-anchor": "top"
      },
      "source": "labels",
      "metadata": {
        "group": "labels-light",
        "position": "top"
      }
    }
  ],
  "id": "osm-liberty"
}