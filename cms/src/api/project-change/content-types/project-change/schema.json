    "country_of_coordination": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::country.country"
    },
    "region_of_interventions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::region.region",
      "mappedBy": "project_changes"
    },
    "country_of_interventions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::country.country",
      "mappedBy": "project_changes"
    },
    "website": {
      "type": "string",
      "required": true
    },
    "project_coordinator_name": {
      "type": "string"
