{
  "kind": "collectionType",
  "collectionName": "practices",
  "info": {
    "singularName": "practice",
    "pluralName": "practices",
    "displayName": "Practice",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "source_name": {
      "type": "enumeration",
      "enum": [
        "WOCAT"
      ],
      "required": true
    },
    "source_id": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "text"
    },
    "short_description": {
      "type": "text"
    },
    "countries": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::country.country",
      "inversedBy": "practices"
    },
    "detailed_description": {
      "type": "text"
    },
    "project_fund": {
      "type": "text"
    },
    "institution_funding": {
      "type": "text"
    },
    "state_province": {
      "type": "text"
    },
    "further_location": {
      "type": "text"
    },
    "map_location": {
      "type": "text"
    },
    "implem_date": {
      "type": "text"
    },
    "publication_date": {
      "type": "date"
    },
    "implem_decade": {
      "type": "text"
    },
    "main_purposes": {
      "type": "text"
    },
    "has_changed": {
      "type": "boolean"
    },
    "land_use_has_changed": {
      "type": "customField",
      "options": {
        "separator": "semicolon"
      },
      "customField": "plugin::string-array.input"
    },
    "degradation_assessed": {
      "type": "customField",
      "options": {
        "separator": "semicolon"
      },
      "customField": "plugin::string-array.input"
    },
    "language": {
      "type": "customField",
      "options": {
        "separator": "semicolon"
      },
      "customField": "plugin::string-array.input"
    },
    "land_use_types": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::land-use-type.land-use-type",
      "inversedBy": "practices"
    },
    "sync": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "practice_url": {
      "type": "text"
    },
    "land_use_priors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::land-use-type.land-use-type",
      "inversedBy": "practices_prior"
    },
    "subinterventions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::subintervention.subintervention",
      "inversedBy": "practices"
    },
    "practice_intervention": {
      "type": "enumeration",
      "enum": [
        "Management",
        "Land Use Change",
        "None"
      ],
      "required": true
    },
    "projects": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::project.project",
      "mappedBy": "practices"
    },
    "organizations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::organization.organization",
      "mappedBy": "practices"
    }
  }
}
