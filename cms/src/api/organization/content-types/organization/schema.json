{
  "kind": "collectionType",
  "collectionName": "organizations",
  "info": {
    "singularName": "organization",
    "pluralName": "organizations",
    "displayName": "Organization",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 150,
      "unique": true
    },
    "organization_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::organization-type.organization-type",
      "required": true
    },
    "organization_type_other": {
      "type": "string"
    },
    "main_organization_theme": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::organization-theme.organization-theme",
      "required": true
    },
    "secondary_organization_theme": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::organization-theme.organization-theme"
    },
    "short_description": {
      "type": "text",
      "required": true,
      "maxLength": 350
    },
    "description": {
      "type": "text",
      "maxLength": 3000
    },
    "country": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::country.country",
      "required": true
    },
    "url": {
      "type": "string",
      "required": true
    },
    "lead_projects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project",
      "mappedBy": "lead_partner"
    },
    "partner_projects": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::project.project",
      "mappedBy": "partners"
    },
    "funded_projects": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::project.project",
      "mappedBy": "funders"
    },
    "publication_status": {
      "type": "enumeration",
      "enum": [
        "proposed",
        "accepted",
        "declined"
      ],
      "default": "accepted",
      "required": true
    },
    "user_email": {
      "type": "email",
      "required": false
    }
  }
}
