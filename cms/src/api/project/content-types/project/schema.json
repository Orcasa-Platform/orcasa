{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "project_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::project-type.project-type",
      "required": true
    },
    "start_date": {
      "type": "date",
      "required": true
    },
    "end_date": {
      "type": "date"
    },
    "short_description": {
      "type": "text",
      "maxLength": 350,
      "required": true
    },
    "country_of_coordination": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::country.country",
      "required": true
    },
    "region_of_interventions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::region.region",
      "mappedBy": "projects"
    },
    "country_of_interventions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::country.country",
      "mappedBy": "projects"
    },
    "website": {
      "type": "string",
      "required": true
    },
    "project_coordinator_email": {
      "type": "string"
    },
    "main_area_of_intervention": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::area-of-intervention.area-of-intervention"
    },
    "main_area_of_intervention_other": {
      "type": "string"
    },
    "secondary_area_of_intervention": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::area-of-intervention.area-of-intervention"
    },
    "third_area_of_intervention": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::area-of-intervention.area-of-intervention"
    },
    "sustainable_development_goals": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sustainable-dev-goal.sustainable-dev-goal"
    },
    "lead_partner": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::organization.organization",
      "inversedBy": "lead_projects",
      "required": true
    },
    "partners": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::organization.organization",
      "inversedBy": "partner_projects"
    },
    "funders": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::organization.organization",
      "inversedBy": "funded_projects"
    },
    "publication_status": {
      "type": "enumeration",
      "enum": [
        "proposed",
        "accepted",
        "declined"
      ],
      "default": "accepted",
      "required": true
    },
    "land_use_types": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::land-use-type.land-use-type",
      "inversedBy": "projects"
    },
    "description": {
      "type": "customField",
      "options": {
        "preset": "custom2"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "practices": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::practice.practice",
      "inversedBy": "projects"
    },
    "project_coordinator_website": {
      "type": "string"
    }
  }
}
